<h1>Create User!</h1>

<form action="<%= users_url %>" method="POST">

  <label for="username">User Name</label>
    <input type="text" 
      id="username" 
      name="user[username]" 
      value="<%= @user.username %>">

      <br>

  <label for="password">Password</label>
    <input type="text"
      name="user[password]"
      value="">

  <br>
  
  <input type="submit" value="Create User">




</form>